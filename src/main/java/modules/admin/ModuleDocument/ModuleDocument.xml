<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://www.skyve.org/xml/document" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="ModuleDocument" xsi:schemaLocation="http://www.skyve.org/xml/document ../../../schemas/document.xsd">
    <documentation><![CDATA[Non-persistent document used to store a selection of a document belonging to a specific
		module. This allows it to be used in a lookupDescription, e.g. Control Panel - Generate 
		Test Data, or when refreshing documents as part of DataMaintenance.]]></documentation>
    <abstract>false</abstract>
    <singularAlias>Module Document</singularAlias>
    <pluralAlias>Module Documents</pluralAlias>
    <audited>true</audited>
    <attributes>
        <boolean trackChanges="true" name="include">
            <documentation><![CDATA[Used to indicate if this document is to be included when refreshing as part of DataMaintenance.]]></documentation>
            <displayName>Include</displayName>
        </boolean>
        <text required="true" trackChanges="true" name="moduleName">
            <displayName>Module Name</displayName>
            <description><![CDATA[The name of the module.]]></description>
            <length>500</length>
        </text>
        <text required="true" trackChanges="true" name="documentName">
            <displayName>Document Name</displayName>
            <description><![CDATA[The name of the document.]]></description>
            <length>500</length>
        </text>
        <text trackChanges="true" name="modDocName">
            <displayName>Document Name</displayName>
            <description><![CDATA[The name of the document.]]></description>
            <length>500</length>
        </text>
    </attributes>
</document>