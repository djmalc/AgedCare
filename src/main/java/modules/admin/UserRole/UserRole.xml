<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://www.skyve.org/xml/document" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="UserRole" xsi:schemaLocation="http://www.skyve.org/xml/document ../../../schemas/document.xsd">
    <abstract>false</abstract>
    <persistent name="ADM_SecurityUserRole"/>
    <singularAlias>User Role</singularAlias>
    <pluralAlias>User Roles</pluralAlias>
    <audited>true</audited>
    <parentDocument databaseIndex="true">User</parentDocument>
    <bizKey expression="{roleName}"/>
    <attributes>
        <text required="true" trackChanges="true" name="roleName">
            <displayName>admin.userRole.roleName.displayName</displayName>
            <domain>variant</domain>
            <length>60</length>
        </text>
    </attributes>
</document>