<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://www.skyve.org/xml/document" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="UserLoginRecord" xsi:schemaLocation="http://www.skyve.org/xml/document ../../../schemas/document.xsd">
    <abstract>false</abstract>
    <persistent name="ADM_UserLoginRecord"/>
    <singularAlias>User Login Record</singularAlias>
    <pluralAlias>User Login Records</pluralAlias>
    <audited>false</audited>
    <iconStyleClass>fa fa-history</iconStyleClass>
    <bizKey expression="{userName} @ {loginDateTime}"/>
    <attributes>
        <text required="true" trackChanges="true" usage="domain" name="userName">
            <displayName>admin.userLoginRecord.userName.displayName</displayName>
            <length>254</length>
        </text>
        <dateTime required="true" trackChanges="true" usage="domain" name="loginDateTime">
            <displayName>admin.userLoginRecord.loginDateTime.displayName</displayName>
        </dateTime>
        <boolean required="true" trackChanges="true" usage="domain" name="failed">
            <displayName>admin.userLoginRecord.failed.displayName</displayName>
        </boolean>
    </attributes>
</document>