<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://www.skyve.org/xml/document" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="DocumentCreator" xsi:schemaLocation="http://www.skyve.org/xml/document ../../../schemas/document.xsd">
    <abstract>false</abstract>
    <singularAlias>Document Creator</singularAlias>
    <pluralAlias>Document Creator</pluralAlias>
    <audited>true</audited>
    <iconStyleClass>fa fa-pencil-square-o</iconStyleClass>
    <bizKey expression="Document Creator"/>
    <attributes>
        <text trackChanges="true" name="outputLocation">
            <displayName>admin.documentCreator.outputLocation.displayName</displayName>
            <description><![CDATA[admin.documentCreator.outputLocation.description]]></description>
            <length>255</length>
        </text>
        <memo trackChanges="true" name="script">
            <displayName>admin.documentCreator.script.displayName</displayName>
        </memo>
        <markup trackChanges="true" name="documentPreview">
            <displayName>admin.documentCreator.documentPreview.displayName</displayName>
        </markup>
        <markup trackChanges="true" name="markdownPreview">
            <displayName>admin.documentCreator.markdownPreview.displayName</displayName>
        </markup>
        <markup trackChanges="true" name="errors">
            <displayName>admin.documentCreator.errors.displayName</displayName>
        </markup>
        <text trackChanges="true" name="defaultModule">
            <displayName>admin.documentCreator.defaultModule.displayName</displayName>
            <description><![CDATA[admin.documentCreator.defaultModule.description]]></description>
            <domain>variant</domain>
            <length>255</length>
        </text>
    </attributes>
    <conditions>
        <condition name="hasErrors">
            <expression><![CDATA[getErrors() != null && getErrors().length() > 0]]></expression>
        </condition>
    </conditions>
</document>